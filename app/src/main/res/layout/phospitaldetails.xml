<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".phospitaldetails"
    android:background="@drawable/gradient_background">


    <TextView
        android:id="@+id/dbeds"
        android:layout_width="270dp"
        android:layout_height="59dp"
        android:layout_marginTop="136dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/doxyavai"
        android:layout_width="270dp"
        android:layout_height="59dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/dbeds"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/dbeds"
        app:layout_constraintTop_toBottomOf="@+id/dbeds" />

    <TextView
        android:id="@+id/dcovpat"
        android:layout_width="270dp"
        android:layout_height="59dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/doxyavai"
        app:layout_constraintStart_toStartOf="@+id/doxyavai"
        app:layout_constraintTop_toBottomOf="@+id/doxyavai" />

    <TextView
        android:id="@+id/ddoctors"
        android:layout_width="270dp"
        android:layout_height="59dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/dcovpat"
        app:layout_constraintStart_toStartOf="@+id/dcovpat"
        app:layout_constraintTop_toBottomOf="@+id/dcovpat" />

    <TextView
        android:id="@+id/dambulance"
        android:layout_width="270dp"
        android:layout_height="59dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/ddoctors"
        app:layout_constraintStart_toStartOf="@+id/ddoctors"
        app:layout_constraintTop_toBottomOf="@+id/ddoctors" />

    <Button
        android:id="@+id/button2"
        android:layout_width="155dp"
        android:layout_height="52dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/hslbutton"
        android:text="Home"
        android:onClick="home"
        app:backgroundTint="@drawable/hslbutton"
        app:layout_constraintEnd_toEndOf="@+id/dambulance"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/dambulance"
        app:layout_constraintTop_toBottomOf="@+id/dambulance" />
</androidx.constraintlayout.widget.ConstraintLayout>